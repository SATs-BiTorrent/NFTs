{
  "remappings": [
    "@openzeppelin/contracts/=lib/openzeppelin-contracts/contracts/",
    "ds-test/=lib/forge-std/lib/ds-test/src/",
    "erc4626-tests/=lib/openzeppelin-contracts/lib/erc4626-tests/",
    "forge-std/=lib/forge-std/src/",
    "openzeppelin-contracts/=lib/openzeppelin-contracts/",
    "@staqeprotocol/v1-core/contracts/=contracts/",
    "@toqen/contracts/=lib/contracts/",
    "@qross/v1-core/contracts/=lib/v1-core/contracts/",
    "@chainlink/contracts-ccip/=lib/v1-core/lib/ccip/contracts/",
    "@chainlink/contracts/=lib/v1-core/lib/ccip/contracts/",
    "ccip/=lib/v1-core/lib/ccip/contracts/",
    "contracts/=lib/contracts/src/",
    "v1-core/=lib/v1-core/"
  ],
  "optimizer": {
    "enabled": true,
    "runs": 200
  },
  "metadata": {
    "useLiteralContent": false,
    "bytecodeHash": "ipfs",
    "appendCBOR": true
  },
  "outputSelection": {
    "*": {
      "*": [
        "evm.bytecode",
        "evm.deployedBytecode",
        "devdoc",
        "userdoc",
        "metadata",
        "abi"
      ]
    }
  },
  "evmVersion": "paris",
  "viaIR": true,
  "libraries": {}
}